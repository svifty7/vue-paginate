{
  "name": "vue-paginate",
  "version": "0.0.1",
  "author": {
    "name": "Nikita Charushin",
    "email": "svifty7@gmail.com",
    "url": "https://github.com/svifty7"
  },
  "keywords": [
    "vue",
    "paginate",
    "pagination"
  ],
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-paginate.umd.js",
  "module": "./dist/vue-paginate.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-paginate.es.js",
      "require": "./dist/vue-paginate.umd.js"
    },
    "./dist/my-library-vue-ts.css": {
      "import": "./dist/vue-paginate.css",
      "require": "./dist/vue-paginate.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "run-s type-check build-only",
    "type-check": "vue-tsc --noEmit",
    "build-only": "vite build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svifty7/vue-paginate.git"
  },
  "bugs": {
    "url": "https://github.com/svifty7/vue-paginate/issues"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitepress": "^1.0.0-alpha.40",
    "vue-tsc": "^1.0.12"
  }
}
