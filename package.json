{
  "name": "vue-paginate",
  "version": "0.0.1",
  "author": {
    "name": "Nikita Charushin",
    "email": "svifty7@gmail.com",
    "url": "https://github.com/svifty7"
  },
  "keywords": [
    "vue",
    "paginate",
    "pagination"
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-paginate.umd.cjs",
  "module": "./dist/vue-paginate.js",
  "exports": {
    ".": {
      "import": "./dist/vue-paginate.js",
      "require": "./dist/vue-paginate.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svifty7/vue-paginate.git"
  },
  "bugs": {
    "url": "https://github.com/svifty7/vue-paginate/issues"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitepress": "^1.0.0-alpha.40",
    "vue-tsc": "^1.0.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": [
      "npm run lint:fix"
    ]
  }
}
